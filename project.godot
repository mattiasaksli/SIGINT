[application]

config/name="SIGINT"
config/description="A cooperative puzzle game for the Raspberry Pi."
run/main_scene="res://scenes/menus/main_menu.tscn"
config/icon="res://.project_meta_files/icon.png"

[autoload]

LeaderboardManager="*res://scripts/leaderboard_manager.gd"
ScreenTransition="*res://scenes/screen_transition.tscn"

[debug]

settings/stdout/print_fps=true
gdscript/warnings/unused_class_variable=true
gdscript/warnings/return_value_discarded=false
gdscript/warnings/unsafe_call_argument=true

[display]

window/size/width=1280
window/size/height=800
window/size/resizable=false
window/vsync/use_vsync=false
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[gdnative]

singletons=[ "res://git_api.gdnlib" ]

